    "fig = plt.figure()\n",
    "ax = plt.axes()\n",
    "plt.plot(company_sales.month_number, company_sales.total_profit,':or',linewidth=3, label='Total Profit Over Time')\n",
    "ax.set(xlim=(1, 12), ylim=(150000, 450000),\n",
    "       xlabel='Month Number', ylabel='Total profit',\n",
    "       title='Total Profit of All months');\n",
    "plt.legend(loc='lower right');"
